(main
 "package main" n n
 "import \"log\"" n n
 "func main() {" n>
 "if err := execute(); err != nil {" n>
 "log.Fatal(err)" n>
 "}" > n>
 "}" > n n
 "func execute() error {" n>
 q n n>
 "return nil" n>
 "}" >
 )

